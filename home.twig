{% extends "layout.twig" %}

{% block content %}

<header>
    <div class="wrapper clearfix header-content">
       <div class="companyheader clearfix">
            {{widget('logo', 'logo', {"defaultlogo": asset('images/logo.png')})|raw}}
            {{widget('companyname', 'companyname', {"business": "profile"})|raw}}
        </div>
        {{widget('extendednavigation', 'navigationheader', {})|raw}}
    </div>
</header>

<section>
    {{widget('feature', 'featurehome', {"header": "Malecena te yeolobo nuncta une rutro pulvinar ", "strapline":"Vivamus vel turpis lacinia, adipiscing eros seda", "buttonText":"MALESUADA ANTE NONTUR","defaultBgImg": asset('images/feature-bg.jpg'), 'positionTopPercentage': '50','showBtn': true})|raw}}
</section>

<div class="main wrapper clearfix">
    {{zone('main')|raw}}

</div>

<footer>
    <div class="wrapper clearfix footer-content">
        <div class="responsivecolumns columns-two-50-50">
            <div class="column columns-two-left">

                 {{widget('profile', 'footercolumn1socialicons', { 'profileType':'socialicons', 'socialicons':'profile'})|raw}}
                
            </div>
            <div class="column columns-two-right">
                <ul>
                    <li>{{widget('profile', 'footercolumn2profilephone', { 'profileType':'phone', 'phone':'profile'})|raw}}</li>
                    <li class="footer-separator">|</li>
                    <li>{{widget('profile', 'footercolumn2profileemail', { 'profileType':'email', 'email':'profile'})|raw}}</li>
                </ul>
                
            </div>
        </div>
    </div>
</footer>


{% endblock %}
